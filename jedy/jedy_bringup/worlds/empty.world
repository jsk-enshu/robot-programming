      <pose>-4 0 0 0 0 1.57102</pose>
    </model>

    <!-- grasp target -->
    <model name="table">
      <static>1</static>
      <pose>1.0 0.0 0.06 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>10</mass>
          <inertia> <!-- inertias are tricky to compute -->
            <ixx>0.1</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
            <ixy>0.0</ixy>         <!-- for a box: ixy = 0 -->
            <ixz>0.0</ixz>         <!-- for a box: ixz = 0 -->
            <iyy>0.015</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
            <iyz>0.0</iyz>         <!-- for a box: iyz = 0 -->
            <izz>0.1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.2 0.4 0.12</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.4</mu>
                <mu2>0.4</mu2>
              </ode>
            </friction>
          </surface>

        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.2 0.4 0.12</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Red</name>
            </script>
          </material>
        </visual>
      </link>
    </model>
    <model name="target_cylinder">
      <pose>1.0 0 0.18 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.01</mass>
          <inertia> <!-- inertias are tricky to compute -->
            <ixx>0.001</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
            <ixy>0.0</ixy>         <!-- for a box: ixy = 0 -->
            <ixz>0.0</ixz>         <!-- for a box: ixz = 0 -->
            <iyy>0.001</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
            <iyz>0.0</iyz>         <!-- for a box: iyz = 0 -->
            <izz>0.001</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.025</radius>
              <length>0.12</length>
            </cylinder>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.9</mu>
                <mu2>0.9</mu2>
              </ode>
            </friction>
          </surface>

        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.025</radius>
              <length>0.12</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Blue</name>
            </script>
          </material>
        </visual>
      </link>
    </model>

    <!-- checkerboard -->
    <model name="checkerboard" >
      <pose>0 1.3 0.3 0 1.5708 -1.5708</pose>
      <include>
        <uri>model://checkerboard</uri>
      </include>
    </model>

    <!-- apriltag -->
    <model name="apriltag" >
      <pose>1.0 0.2 0.3 0 1.5708 0</pose>
      <include>
        <uri>model://Apriltag36_11_00000</uri>
      </include>
    </model>

  </world>
